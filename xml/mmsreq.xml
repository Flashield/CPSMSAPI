<?xml version="1.0" encoding="utf-8" ?>
<ipf>
	<header>
		<!-- 请求标记，唯一确定本次请求 -->
		<transactionId>10011001</transactionId>
		<!-- 用户名，鉴权使用，由平台分配 -->
		<user>test</user>
		<!-- 密码，鉴权使用，由平台分配，MD5加密 -->
		<passwd>123456</passwd>
		<!-- 包体类型 -->
		<packageType>MMSMTRequest</packageType>
	</header>
	<body>
		<!-- 活动代码，由平台分配 -->
		<actId>SMS2MMS</actId>
		<!-- 发送方手机号码 -->
		<feeTerminalId>13813388666</feeTerminalId>
		<!-- 接收方手机号码 -->
		<destTerminalId>13813388666</destTerminalId>
		<!-- 抄送方手机号码 -->
		<ccTerminalId>13813388777,13813388787</ccTerminalId>
		<!-- 业务代码，由平台分配 -->
		<serviceId>12345678</serviceId>
		<!-- 费率，以分为单位，由平台分配 -->
		<fee>100</fee>
		<!-- 是否需要状态报告：0、不需要，1、需要 -->
		<needReport>0</needReport>
		<!-- 彩信标题 -->
		<msgTitle>XXXX</msgTitle>
		<!-- 彩信内容 -->
		<msgContent>base64加密zip包后的文本</msgContent>
		<!-- Linkid，点播类相应于上行 -->
		<linkid>0123456789</linkid>
	</body>
</ipf>